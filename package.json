{
  "name": "is-satire",
  "version": "1.0.13",
  "description": "Strategically determine if a website is satire or not",
  "main": "lib/index.js",
  "scripts": {
    "test-ci": "babel-node test/index.js",
    "test": "babel-node test/index.js | tap-notify | faucet",
    "transform": "babel -d lib/ src/",
    "build": "mkdirp build && browserify --entry ./index.js --standalone isSatire --outfile build/is-satire.js -t [ babelify --presets [ es2015 ] ]",
    "lint": "eslint --cache src/",
    "cli": "babel-node src/cli.js",
    "prepublish": "npm run lint && npm run transform && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moimikey/is-satire.git"
  },
  "keywords": [
    "herald",
    "news",
    "publishing",
    "the onion",
    "satire",
    "satirical",
    "spoof",
    "fake",
    "humor",
    "humour",
    "exaggeration",
    "exaggerate",
    "mock",
    "hoax",
    "fiction",
    "parody",
    "sat√≠rico"
  ],
  "author": "Michael Scott Hertzberg <mshertzberg@gmail.com> (http://hertzber.gs/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moimikey/is-satire/issues"
  },
  "homepage": "https://github.com/moimikey/is-satire",
  "preferGlobal": true,
  "bin": {
    "is-satire": "./cli.js"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "6.5.1",
    "babel-eslint": "5.0.0-beta8",
    "babel-plugin-external-helpers": "6.5.0",
    "babel-plugin-transform-runtime": "6.5.2",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "eslint": "1.10.3",
    "faucet": "0.0.1",
    "mkdirp": "0.5.1",
    "tap-notify": "0.0.3",
    "tap-spec": "4.1.1",
    "tape": "4.4.0"
  },
  "dependencies": {
    "babel-runtime": "6.5.0",
    "bluebird": "3.2.2",
    "cheerio": "0.20.0",
    "hasdom": "0.0.4",
    "node-fetch": "1.3.3",
    "url": "0.11.0"
  }
}
